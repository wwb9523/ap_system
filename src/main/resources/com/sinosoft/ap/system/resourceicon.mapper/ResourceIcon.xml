<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sinosoft.ap.system.resourceicon.domain.ResourceIconRepository">

	<insert id="insert" parameterType="com.sinosoft.ap.system.resourceicon.domain.ResourceIconVO">
		insert into APS_RESOURCE_ICON(RESOURCE_ICON_ID,RESOURCE_ICON_NAME,RESOURCE_ICON_ADDRESS) values(#{resourceIcon.RESOURCE_ICON_ID},#{resourceIcon.RESOURCE_ICON_NAME},
		#{resourceIcon.RESOURCE_ICON_ADDRESS})
	</insert>

	<delete id="delete" parameterType="com.sinosoft.ap.system.resourceicon.domain.ResourceIconVO">
		delete from APS_RESOURCE_ICON where RESOURCE_ICON_ID=#{resourceIcon.RESOURCE_ICON_ID}
	</delete>

</mapper>