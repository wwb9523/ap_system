<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sinosoft.ap.system.library.domain.LibraryRepository">

	<select id="findLibraryById" parameterType="java.lang.String" resultType="com.sinosoft.ap.system.library.domain.LibraryEntity">
		SELECT * FROM APS_LIBRARY where LIBRARY_ID = #{id}
	</select>


</mapper>